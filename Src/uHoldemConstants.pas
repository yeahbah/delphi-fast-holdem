class function THoldemConstants.BitCount(aBitField: uint64): integer;
begin
  result := BitsAndStrTable[aBitField and $1FFFF] +
            BitsAndStrTable[aBitField shr 13] +
            BitsAndStrTable[aBitField shr 16] +
            BitsAndStrTable[aBitField shr 39];
end;

class function THoldemConstants.CardRank(aCard: integer): integer;
begin
  {$IFDEF DEBUG}
  if (aCard < 0) or (aCard > 52) then raise EArgumentException.Create('Invalid card');
  {$ENDIF}

  result := aCard mod 13;
end;

class function THoldemConstants.CardSuit(aCard: integer): integer;
begin
  {$IFDEF DEBUG}
  if (aCard < 0) or (aCard > 52) then raise EArgumentException.Create('Invalid card');
  {$ENDIF}

  result := aCard div 13;
end;

